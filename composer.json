{
  "name": "dualmedia/symfony-request-dto-bundle",
  "type": "symfony-bundle",
  "description": "Symfony bundle which provides DTO object functionality and resolving",
  "keywords": [
    "dto",
    "symfony",
    "request"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "pkly",
      "homepage": "https://github.com/pkly",
      "role": "Author"
    }
  ],
  "require": {
    "php": "^8.3",
    "symfony/framework-bundle": "^6.4|^7.3",
    "symfony/event-dispatcher": "^6.4|^7.3",
    "symfony/property-info": "^6.4|^7.3",
    "symfony/validator": "^6.4|^7.3",
    "symfony/property-access": "^6.4|^7.3"
  },
  "require-dev": {
    "ext-simplexml": "*",
    "phpunit/phpunit": "^12",
    "friendsofphp/php-cs-fixer": "^3",
    "pedrotroller/php-cs-custom-fixer": "^2",
    "vimeo/psalm": "^6",
    "phpstan/phpstan": "^2",
    "symfony/phpunit-bridge": "^6.4|^7.3",
    "matthiasnoback/symfony-dependency-injection-test": "^6",
    "nelmio/api-doc-bundle": "^5",
    "symfony/stopwatch": "^6.4|^7.3",
    "symfony/expression-language": "^6.4|^7.3",
    "symfony/web-profiler-bundle": "^6.4|^7.3",
    "doctrine/doctrine-bundle": "^2",
    "doctrine/orm": "^2|^3",
    "phpstan/phpstan-strict-rules": "^2.0"
  },
  "conflict": {
    "nelmio/api-doc-bundle": "<5",
    "doctrine/doctrine-bundle": "<2.4"
  },
  "autoload": {
    "psr-4": {
      "DualMedia\\DtoRequestBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DualMedia\\DtoRequestBundle\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "lint": "php-cs-fixer fix",
    "test": "phpunit",
    "baseline-psalm": "vendor/bin/psalm --set-baseline=psalm-baseline.xml",
    "baseline-phpstan": "vendor/bin/phpstan --generate-baseline"
  },
  "suggest": {
    "nelmio/api-doc-bundle": "Adds api docs for endpoints for your DTOs",
    "doctrine/doctrine-bundle": "Symfony's ORM of choice"
  },
  "extra": {
    "symfony": {
      "require": "6.4.*",
      "allow-contrib": false
    }
  }
}
